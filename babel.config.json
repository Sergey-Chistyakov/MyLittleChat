{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    [
      "@babel/preset-typescript",
      {
        "extensions": ".ts",
        "isTSX": true,
        "allExtensions": true
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-transform-remove-imports",
      {
        "test": [
          "react"
        ]
      }
    ]
  ]
}